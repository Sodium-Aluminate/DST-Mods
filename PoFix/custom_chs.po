#. STRINGS.CHARACTERS.WORMWOOD.DESCRIBE.SINGINGSHELL_OCTAVE5.GENERIC
msgctxt "STRINGS.CHARACTERS.WORMWOOD.DESCRIBE.SINGINGSHELL_OCTAVE5.GENERIC"
msgid "It sings"
msgstr "它能唱\n（{note}）"

#. STRINGS.CHARACTERS.WORTOX.DESCRIBE.SINGINGSHELL_OCTAVE3.GENERIC
msgctxt "STRINGS.CHARACTERS.WORTOX.DESCRIBE.SINGINGSHELL_OCTAVE3.GENERIC"
msgid "A barnacled baritone!"
msgstr "贝壳男中音！\n（{note}）"

#. STRINGS.CHARACTERS.WORTOX.DESCRIBE.SINGINGSHELL_OCTAVE4.GENERIC
msgctxt "STRINGS.CHARACTERS.WORTOX.DESCRIBE.SINGINGSHELL_OCTAVE4.GENERIC"
msgid "Sweet singing seashells!"
msgstr "甜美的唱歌贝壳！\n（{note}）"

#. STRINGS.CHARACTERS.WORTOX.DESCRIBE.SINGINGSHELL_OCTAVE5.GENERIC
msgctxt "STRINGS.CHARACTERS.WORTOX.DESCRIBE.SINGINGSHELL_OCTAVE5.GENERIC"
msgid "Such a high pitch, it makes my ears itch..."
msgstr "曲高和寡，温文尔雅……\n（{note}）"

#. STRINGS.CHARACTERS.WURT.DESCRIBE.SINGINGSHELL_OCTAVE3.GENERIC
msgctxt "STRINGS.CHARACTERS.WURT.DESCRIBE.SINGINGSHELL_OCTAVE3.GENERIC"
